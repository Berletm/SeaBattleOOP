# Change to directory where compile_commands.json is
compilation_database_dir: .
# Change to directory where diagram should be written
output_directory: diagrams
diagrams:
  full-diagram:
    type: class
    glob:
      - src/other/*.cpp
      - src/other/*.hpp
      - src/ability/*.cpp
      - src/ability/*.hpp
  abilties-diagram:
    type: class
    glob:
      - src/ability/*.cpp
      - src/ability/*.hpp
    include:
      context:
        - AbilityInterface
        - AbilityManager
        - AbilityRegistry
